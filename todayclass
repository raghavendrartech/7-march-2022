interface Payment
{
  public void pay();
}

class Main
{
  public static void main(String args[])
  {
    double amount=100.0;

 //Implementation without lambda expression, using anonymous class 
  Payment p=new Payment(){
    
	public void pay()
 	{
 		System.out.println("Collect your payment of Rs. "+amount);
	}
   };

	p.pay();
    
   }


}
